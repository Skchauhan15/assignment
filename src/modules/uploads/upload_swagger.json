{
    "openapi": "3.0.3",
    "info": {
        "title": "express project Documentation",
        "description": "express project Documentation",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:5000/",
            "description": "local server"
        }
    ],
    "tags": [
        {
            "name": "Upload",
            "description": "Upload api"
        }
    ],
     "paths": {
         "/upload/upload_file": {
            "post": {
                "tags": [
                    "Upload"
                ],
                "summary": "Upload file Api",
                "requestBody": {
                    "content": {
                         "multipart/form-data": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "file": {
                                        "type": "string",
                                        "format": "binary"
                                    }
                                },
                                "required": [
                                    "file"
                                ]
                            }
                        }
                    }
                },
                "responses": {
                    "default": {
                        "description": "successful operation"
                    }
                },
                "security": [
                    {
                      "access_token": ["authorization"]
                    }
                ]
            }
        }
     },
     "components":{
        "securitySchemes": {
            "access_token": {
              "type": "http",
              "scheme": "bearer",
              "bearerFormat": "JWT"
             }
           }
     }
}
